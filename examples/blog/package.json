{
  "name": "blog",
  "description": "File based routing and MDX support Vite.js tooling. Prettier code formatter.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.ssr.tsx",
    "dev": "npm run dev.ssr",
    "dev.client": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --force --mode ssr",
    "dev.ssr": "VITE_NODE_STINKS=true node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "start": "npm run dev",
    "typecheck": "tsc --incremental --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.5",
    "@builder.io/qwik-city": "^1.1.5",
    "@types/eslint": "8.40.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.41.0",
    "eslint-plugin-qwik": "^1.1.5",
    "node-fetch": "3.2.10",
    "prettier": "2.8.8",
    "typescript": "4.7.4",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "type": "module"
}
